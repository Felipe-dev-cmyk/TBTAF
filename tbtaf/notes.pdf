CREATE TABLE test_suite(
    id NUMBER GENERATED BY DEFAULT AS IDENTITY,
    suite_type VARCHAR2(50) NOT NULL,
    suite_id VARCHAR2(50) NOT NULL,
    total_test int,
    passed_test int,
    failed_test int,
    inconclusive_test int,
    success_rate int,
    start_time DATE,
    end_time DATE,
    time_elapse VARCHAR2(50),
    PRIMARY KEY(id)
);


CREATE TABLE test(
    id NUMBER GENERATED BY DEFAULT AS IDENTITY,
    test_id VARCHAR2(50) NOT NULL,
    description VARCHAR2(250),
    tags VARCHAR2(250),
    priority int,
    source VARCHAR2(250),
    start_time DATE,
    end_time DATE,
    time_elapse VARCHAR2(50),
    veredict VARCHAR2(50) NOT NULL,
    suite_id int,
    PRIMARY KEY(id)
);

